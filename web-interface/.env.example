# Hedera Network Configuration
# Options: 'testnet' or 'mainnet' (default: testnet)
NEXT_PUBLIC_HEDERA_NETWORK=testnet

# WalletConnect Project ID
# Get your project ID from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_ID=your_walletconnect_project_id_here


# Contract Addresses (update with your deployed contracts)
# Testnet Contracts
NEXT_PUBLIC_TESTNET_SWARM_ORCHESTRATOR=0.0.1234567
NEXT_PUBLIC_TESTNET_EVOLUTION_ENGINE=0.0.1234568

# Mainnet Contracts (when deployed)
NEXT_PUBLIC_MAINNET_SWARM_ORCHESTRATOR=
NEXT_PUBLIC_MAINNET_EVOLUTION_ENGINE=